{
  "name": "ljournal",
  "version": "2.0.0",
  "private": true,
  "description": "Blog and Journal Site",
  "author": "Brian Lusina <lusinabrian@gmail.com>",
  "license": "The MIT License (MIT)",
  "homepage": "https://brianlusina.github.io",
  "scripts": {
    "commit": "git-cz",
    "build:prod": "NODE_ENV=production gatsby build",
    "build:staging": "NODE_ENV=staging gatsby build",
    "start": "gatsby develop",
    "format": "prettier --trailing-comma es5 --no-semi --single-quote --write \"src/**/*.js\"",
    "test": "node scripts/test.js --env=jsdom",
    "test:cover": "npm run test -- --coverage",
    "prepublish:prod": "npm run build:prod",
    "publish:prod": "gh-pages -d public",
    "prepublish:staging": "npm run build:staging",
    "publish:staging": "surge --project ./public --domain ljournal.surge.sh",
    "serve": "http-server ./public -p 8080",
    "snyk-protect": "snyk protect",
    "prepare": "yarn run snyk-protect"
  },
  "dependencies": {
    "apollo-boost": "^0.1.23",
    "gatsby": "^2.0.78",
    "gatsby-plugin-google-analytics": "^2.0.8",
    "gatsby-plugin-manifest": "^2.2.21",
    "gatsby-plugin-offline": "^2.0.20",
    "gatsby-plugin-paginate": "^1.12.0",
    "gatsby-plugin-react-helmet": "3.0.12",
    "gatsby-plugin-sass": "2.0.11",
    "gatsby-source-filesystem": "^2.0.12",
    "gatsby-source-graphql": "^2.0.8",
    "gatsby-transformer-json": "^2.1.6",
    "gatsby-transformer-remark": "^2.1.17",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.17.13",
    "moment": "^2.23.0",
    "prop-types": "^15.6.2",
    "react": "^16.7.0",
    "react-apollo": "^2.3.3",
    "react-dom": "^16.7.0",
    "react-helmet": "^5.2.0",
    "react-markdown": "^4.0.4",
    "react-redux": "^6.0.0",
    "react-router-dom": "^4.3.1",
    "redux": "^4.0.1",
    "redux-thunk": "^2.3.0",
    "snyk": "^1.744.0"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.3",
    "@babel/plugin-proposal-decorators": "^7.2.3",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "7.2.0",
    "@commitlint/cli": "^7.3.1",
    "@commitlint/config-conventional": "^7.1.2",
    "babel-core": "^7.0.0-0",
    "babel-jest": "^24.0.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-plugin-transform-decorators": "^6.24.1",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "chai": "^4.2.0",
    "codecov": "^3.6.5",
    "commitizen": "^4.0.4",
    "dotenv-expand": "^4.2.0",
    "enzyme": "^3.8.0",
    "enzyme-adapter-react-16": "^1.7.1",
    "eslint": "5.11.1",
    "eslint-config-react-app": "^3.0.6",
    "eslint-loader": "2.1.1",
    "eslint-plugin-flowtype": "3.2.0",
    "eslint-plugin-import": "2.14.0",
    "eslint-plugin-jsx-a11y": "6.1.2",
    "eslint-plugin-react": "7.12.0",
    "expect": "^24.0.0",
    "faker": "^4.1.0",
    "gatsby-cli": "^2.12.14",
    "http-server": "^0.12.0",
    "husky": "^1.3.1",
    "identity-obj-proxy": "^3.0.0",
    "imagemin-lint-staged": "^0.3.1",
    "jest": "24.0.0",
    "jsdoc": "^3.6.0",
    "lint-staged": "^8.1.0",
    "nock": "^10.0.5",
    "node-sass": "^4.13.1",
    "npm-run-all": "^4.1.5",
    "prettier": "^1.15.3",
    "raf": "^3.4.1",
    "react-dev-utils": "^11.0.4",
    "redux-devtools-extension": "^2.13.7",
    "redux-immutable-state-invariant": "^2.1.0",
    "rimraf": "^2.6.2",
    "sassdoc": "^2.5.1",
    "semantic-release": "^17.2.3",
    "sinon": "^7.2.2",
    "surge": "^0.20.2",
    "sw-precache-webpack-plugin": "0.11.5"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "npm run test -- --watchAll=false --onlyChanged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "linters": {
      "*.{js,jsx}": [
        "eslint --fix",
        "git add"
      ],
      "*.{png,jpeg,jpg,gif,svg}": [
        "imagemin-lint-staged",
        "git add"
      ]
    },
    "ignore": [
      "**/build/*.min.js",
      "build/",
      "public/",
      "**/scripts/**/*.js",
      "**/config/**/*.js"
    ]
  },
  "snyk": true
}
